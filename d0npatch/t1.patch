-                        String[] data = (String[]) ar.result;
-                        if (data.length > 0) {
-                            parseSmsSgsValue(data[0]);
-                            return;
-                        }
-                    }
-                }
-                showToast("Query failed");
-            } else if (msg.what == MSG_SET) {
