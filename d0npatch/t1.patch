-    public static int newGetCurrent(int hostNumber, int opcode) {
-        String response = runCmdInNative(
-                AFMFunctionCallEx.FUNCTION_EM_MSDC_GET_CURRENT, 2, hostNumber, opcode);
-        Log.d("@M_" + TAG, "newGetCurrent: " + response);
-        int idx = -1;
