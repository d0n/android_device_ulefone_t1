-                    int msgLen = DataConvert.byteToInt(lenght);
-                    Elog.d(NfcMainPage.TAG, "[ReceiveThread:info]Recieved data message lenght is "
-                            + msgLen);
-                    NfcCommand mainMessage;
-                    if (msgLen == 0) {
